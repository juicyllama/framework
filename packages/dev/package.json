{
	"name": "@juicyllama/dev",
	"version": "0.0.54",
	"description": "Juicyllama Framework Development Dependencies",
	"author": {
		"name": "JuicyLlama Studio",
		"email": "studio@juicyllama.com",
		"url": "https://juicyllama.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/juicyllama-npm/dev"
	},
	"bugs": "https://github.com/juicyllama-npm/dev",
	"license": "0BSD",
	"readmeFilename": "README.md",
	"main": "index.js",
	"files": [
		"dist/**/*",
		"*.md",
		"*.js",
		"*.ts",
		"index.ts"
	],
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "rm -rf dist && tsc",
		"format": "prettier --write .",
		"lint": "eslint --fix",
		"test": "jest --runInBand --detectOpenHandles --forceExit",
		"release": "release-it --ci",
		"release:minor": "release-it minor --ci",
		"release:major": "release-it major --ci"
	},
	"dependencies": {
		"@babel/preset-typescript": "^7.21.0",
		"@faker-js/faker": "^7.6.0",
		"@nestjs/devtools-integration": "^0.1.3",
		"@nestjs/testing": "9.4.0",
		"@types/cron": "^2.0.1",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.1",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.16.0",
		"@types/passport": "^1.0.11",
		"@types/passport-http": "^0.3.9",
		"@types/passport-jwt": "^3.0.8",
		"@types/passport-local": "^1.0.35",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.49.1",
		"@typescript-eslint/parser": "^5.49.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"i": "^0.3.7",
		"jest": "^29.4.3",
		"jest-environment-jsdom": "^29.4.3",
		"npm": "^9.6.5",
		"prettier": "^2.8.8",
		"release-it": "^15.6.0",
		"supertest": "^6.3.3",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"tsc-watch": "6.0.0",
		"tsconfig-paths": "^4.1.2"
	},
	"release-it": {
		"hooks": {
			"before:init": [
				"npm run format",
				"npm run lint",
				"npm run test"
			],
			"after:release": "echo âœ… Successfully released ${name} v${version} to ${npm.name}."
		},
		"npm": {
			"publish": true
		},
		"github": {
			"release": false
		},
		"git": {
			"commit": true,
			"tag": true,
			"push": true,
			"requireCleanWorkingDir": false
		}
	}
}
