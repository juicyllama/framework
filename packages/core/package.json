{
    "name": "@juicyllama/core",
    "version": "0.0.299",
    "description": "Juicyllama NestJS Core",
    "author": {
        "name": "JuicyLlama Studio",
        "email": "studio@juicyllama.com",
        "url": "https://juicyllama.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/juicyllama-npm/core"
    },
    "bugs": "https://github.com/juicyllama-npm/core",
    "license": "0BSD",
    "readmeFilename": "README.md",
    "main": "dist/index.js",
    "files": [
        "dist/**/*",
        "*.md"
    ],
    "types": "dist/index.d.ts",
    "scripts": {
        "sandbox": "nest start --config src/sandbox/nest-cli.json --watch ",
        "prebuild": "rimraf dist",
        "build": "tsc",
        "prepare": "npm run build",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "test": "npm run prebuild && jest --runInBand --detectOpenHandles --forceExit --bail",
        "test:users": "npm run prebuild && jest --runInBand --detectOpenHandles --forceExit --bail --testPathPattern=src/modules/users/users.controller.test.spec.ts",
        "test:charts": "npm run prebuild && jest --runInBand --detectOpenHandles --forceExit --bail --testPathPattern=src/modules/users/users.controller.charts.test.spec.ts",
        "test:filtering": "npm run prebuild && jest --runInBand --detectOpenHandles --forceExit --bail --testPathPattern=src/utils/typeorm/Query.filtering.test.spec.ts",
        "test:clear": "npx jest --clearCache",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:clearCache": "jest --clearCache",
        "prerelease": "npm run prebuild && npm run format && npm run lint && npm run test && npm run build",
        "release": "release-it --ci",
        "release:minor": "release-it minor --ci",
        "release:major": "release-it major --ci",
        "releaseit:skip": "release-it --ci"
    },
    "dependencies": {
        "@bugsnag/js": "^7.20.0",
        "@bugsnag/plugin-express": "^7.19.0",
        "@juicyllama/dev": "workspace:*",
        "@juicyllama/nestjs-redoc": "latest",
        "@juicyllama/utils": "workspace:*",
        "@nestjs/cli": "^9.4.2",
        "@nestjs/common": "^9.4.0",
        "@nestjs/config": "^2.3.1",
        "@nestjs/core": "^9.4.0",
        "@nestjs/jwt": "^10.0.3",
        "@nestjs/mapped-types": "^1.2.0",
        "@nestjs/microservices": "^9.4.0",
        "@nestjs/passport": "^9.0.3",
        "@nestjs/platform-express": "^9.4.0",
        "@nestjs/schedule": "^2.2.2",
        "@nestjs/schematics": "^9.1.0",
        "@nestjs/serve-static": "^3.0.1",
        "@nestjs/swagger": "^6.3.0",
        "@nestjs/testing": "^9.4.0",
        "@nestjs/typeorm": "^9.0.1",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.16.0",
        "amqp-connection-manager": "^4.1.12",
        "amqplib": "^0.10.3",
        "cache-manager": "^5.2.1",
        "cache-manager-redis-store": "^3.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "csv-parser": "^3.0.0",
        "dotenv": "^16.0.3",
        "joi": "^17.9.2",
        "lodash": "^4.17.21",
        "module-alias": "^2.2.2",
        "mongodb": "^5.3.0",
        "mysql2": "^3.2.3",
        "nestjs-command": "^3.1.3",
        "nestjs-real-ip": "^2.2.0",
        "passport": "^0.6.0",
        "passport-azure-ad": "^4.3.5",
        "passport-facebook": "^3.0.0",
        "passport-github": "^1.1.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-headerapikey": "^1.2.2",
        "passport-http": "^0.3.0",
        "passport-instagram": "^1.0.0",
        "passport-jwt": "^4.0.1",
        "passport-linkedin-oauth2": "^2.0.0",
        "passport-local": "^1.0.0",
        "passport-microsoft": "^1.0.0",
        "passport-oauth": "^1.0.0",
        "pusher": "^5.1.2",
        "qs": "^6.11.1",
        "redis": "^4.6.5",
        "reflect-metadata": "^0.1.13",
        "source-map-support": "^0.5.21",
        "swagger-ui-express": "^4.6.2",
        "typeorm": "^0.3.15",
        "typescript": "^5.0.4"
    },
    "eslintConfig": {
        "extends": [
            "./node_modules/@juicyllama/dev/.eslintrc.js"
        ]
    },
    "prettier": "./node_modules/@juicyllama/dev/.prettierrc.js",
    "release-it": {
        "hooks": {
            "after:release": "echo âœ… Successfully released ${name} v${version} to ${npm.name}."
        },
        "npm": {
            "publish": true,
            "timeout": 100000
        },
        "github": {
            "release": true,
            "timeout": 100000
        },
        "git": {
            "commit": true,
            "tag": true,
            "push": true,
            "requireCleanWorkingDir": false
        }
    },
    "devDependencies": {
        "moment": "^2.29.4"
    }
}
